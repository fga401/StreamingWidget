        <Label x:Name="KALabelN" Content="20(10)" HorizontalAlignment="Right" VerticalAlignment="Top" Background="{x:Null}" Foreground="White" FontFamily="./Resources/#SplatFont 2" FontSize="30" Margin="0,120,196,0">
